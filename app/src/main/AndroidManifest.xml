<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.recouture">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartUpPage.SplashActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity android:name=".StartUpPage.LoginActivity">
        </activity>

        <activity android:name=".StartUpPage.SignUpActivity">
        </activity>

        <activity android:name="com.example.recouture.HomepageActivity">
        </activity>

        <activity android:name="com.example.recouture.ProfileActivity">
        </activity>


    </application>

</manifest>