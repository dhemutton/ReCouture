<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ADC6F0"
    tools:context=".Splash">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="111dp"
        android:layout_height="118dp"
        app:srcCompat="@drawable/logo"
        android:translationX="137dp"
        android:translationY="157dp"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="157dp" />



    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="142dp"
        android:layout_height="49dp"
        app:srcCompat="@drawable/logo2"
        android:translationY="275dp"
        android:translationX="121dp"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="275dp" />



    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="186dp"
        android:layout_height="26dp"
        app:srcCompat="@drawable/tag"
        android:translationX="99dp"
        android:translationY="316dp"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="316dp" />

</RelativeLayout>